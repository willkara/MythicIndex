{
	"name": "mythicindex-frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "wrangler pages dev --d1=DB -- vite dev",
		"dev:vite": "vite dev",
		"build": "vite build",
		"deploy": "npm run build && npx wrangler pages deploy .svelte-kit/cloudflare --project-name mythicindex",
		"preview": "wrangler pages dev --d1=DB -- vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint 'src/**/*.ts'",
		"format": "prettier --write 'src/**/*.{ts,svelte}'",
		"format:check": "prettier --check 'src/**/*.{ts,svelte}'",
		"ingest": "npx tsx src/lib/server/ingestion/cli/ingest.ts",
		"ingest:dry": "npx tsx src/lib/server/ingestion/cli/ingest.ts --dry-run"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20251202.0",
		"@eslint/js": "^9.17.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"autoprefixer": "^10.4.22",
		"better-sqlite3": "^12.5.0",
		"dotenv": "^17.2.3",
		"drizzle-kit": "^0.31.7",
		"drizzle-orm": "^0.44.7",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^10.1.8",
		"postcss": "^8.5.6",
		"prettier": "^3.4.2",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^3.4.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.19.1",
		"vite": "^7.2.2",
		"wrangler": "^4.51.0"
	},
	"dependencies": {
		"@clack/prompts": "^0.11.0",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@tiptap/core": "^3.14.0",
		"@tiptap/extension-placeholder": "^3.14.0",
		"@tiptap/pm": "^3.14.0",
		"@tiptap/starter-kit": "^3.14.0",
		"@types/handlebars": "^4.0.40",
		"@types/uuid": "^10.0.0",
		"bits-ui": "^1.3.11",
		"clsx": "^2.1.1",
		"commander": "^14.0.0",
		"handlebars": "^4.7.8",
		"isomorphic-dompurify": "^2.34.0",
		"lucide-svelte": "^0.469.0",
		"marked": "^17.0.1",
		"mode-watcher": "^0.5.1",
		"picocolors": "^1.1.1",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^0.3.1",
		"uuid": "^13.0.0",
		"yaml": "^2.8.2",
		"zod": "^3.22.3"
	}
}
